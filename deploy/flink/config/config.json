{
 "name": "config",
 "sink": {
  "name": "sinkname",
  "type": "kafka",
  "config": {
   "brokers": [
    "kafka:9092"
   ],
   "topic": "topic-A"
  }
 },
 "source": {
  "name": "sourcename",
  "timestamp_field": "t",
  "batch_size": 2,
  "concurrency": 2,
  "interval": [
   1000000,
   2000000
  ],
  "fields": [
   {
    "name": "t",
    "type": "timestamp"
   },
   {
    "name": "name",
    "type": "string",
    "range": [
     "Jon",
     "Ava",
     "Pino"
    ]
   },
   {
    "name": "country",
    "type": "string",
    "range": [
     "USA",
     "Canada",
     "China"
    ]
   },
   {
    "name": "age",
    "type": "int",
    "limit": [
     18,
     50
    ]
   }
  ]
 }
}